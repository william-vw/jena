[simulPolicy:
(?p, rdf:type, cig:SimulTasksPolicy) & 
	(?p cig:intPt ?t) & (?t rdf:type cig:Candidate) & (?t cig:startTime ?st) & 
	within(?st, ?su, cig:X) &
	(?p cig:intPt ?u) & (?u rdf:type cig:Candidate) & (?u cig:startTime ?su) &
	notEqual(?t, ?u) & 
	within(?st, ?su, cig:Y)

-> (?t cig:simulTask ?u)
]